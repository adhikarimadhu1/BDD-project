package functions;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class secondPageFunctions extends CommonFunctions  {
	public secondPageFunctions(WebDriver driver) {
		super(driver);
	}
	private By basic = By.id("grid-row-0-fare-cell-desktop-BE");
	
    public void selectBasic() {
    	WebElement clickBasic = driver.findElement(basic);
    }
}
